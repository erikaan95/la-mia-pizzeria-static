      <section id="tavolo" class="bg-danger">
        <div class="container p-4">
          <h2 class="text-center">Prenota un tavolo</h2>
          <form id="offerForm" class="row gy-2 my-3">
            <!-- Nome input-->
            <div class="col-sm-6 col-12">
                <label class="form-label" for="first-name"><strong>Nome</strong></label>
                <input class="form-control" id="first-name" type="text" placeholder="Inserisci il tuo nome..." required />
            </div>
            <!-- Cognome input -->
            <div class="col-sm-6 col-12">
              <label class="form-label" for="name"><strong>Cognome</strong></label>
              <input class="form-control" id="name" type="text" placeholder="Inserisci il tuo cognome..." required />
            </div>
            <!-- Email input-->
            <div class="col-sm-6 col-12">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-envelope-heart" viewBox="0 0 16 16">
  <path fill-rule="evenodd" d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V4Zm2-1a1 1 0 0 0-1 1v.217l3.235 1.94a2.76 2.76 0 0 0-.233 1.027L1 5.384v5.721l3.453-2.124c.146.277.329.556.55.835l-3.97 2.443A1 1 0 0 0 2 13h12a1 1 0 0 0 .966-.741l-3.968-2.442c.22-.28.403-.56.55-.836L15 11.105V5.383l-3.002 1.801a2.76 2.76 0 0 0-.233-1.026L15 4.217V4a1 1 0 0 0-1-1H2Zm6 2.993c1.664-1.711 5.825 1.283 0 5.132-5.825-3.85-1.664-6.843 0-5.132Z"/>
</svg>
                <label class="form-label" for="email"><strong>Emai</strong>l</label>
                <input class="form-control" id="email" type="email" placeholder="name@example.com" required />       
            </div>
            <!-- cellulare input-->
            <div class="col-sm-6 col-12">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-phone" viewBox="0 0 16 16">
  <path d="M11 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h6zM5 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H5z"/>
  <path d="M8 14a1 1 0 1 0 0-2 1 1 0 0 0 0 2z"/>
</svg>
                <label class="form-label" for="hours"><strong>Cellulare</strong></label>
                <input class="form-control" id="hours" type="text" placeholder="Inserisci il tuo numero cellulare..."/>      
            </div>
               <!-- Data -->
            <div class="col-sm-6 col-12">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-calendar-heart" viewBox="0 0 16 16">
                   <path fill-rule="evenodd" d="M4 .5a.5.5 0 0 0-1 0V1H2a2 2 0 0 0-2 2v11a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V3a2 2 0 0 0-2-2h-1V.5a.5.5 0 0 0-1 0V1H4V.5ZM1 14V4h14v10a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1Zm7-6.507c1.664-1.711 5.825 1.283 0 5.132-5.825-3.85-1.664-6.843 0-5.132Z"/>
                   </svg>
              <label class="form-label" for="phone"><strong>Data</strong></label>
              <input class="form-control" id="data" type="text"/>      
            </div>
            <!-- fascia oraria-->
            <div class="col-sm-6 col-12">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clock" viewBox="0 0 16 16">
  <path d="M8 3.5a.5.5 0 0 0-1 0V9a.5.5 0 0 0 .252.434l3.5 2a.5.5 0 0 0 .496-.868L8 8.71V3.5z"/>
  <path d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm7-8A7 7 0 1 1 1 8a7 7 0 0 1 14 0z"/>
</svg>
              <label class="form-label" for="fascia-oraria"><strong>Orario</strong></label>
              <select class="form-select" id="type-of-work">
                <option selected value="0">Seleziona la fascia oraria</option>
                <option value="1">18:30 - 19:30</option>
                <option value="2">19:30 - 20:30</option>
                <option value="3">20:30 - 21:30</option>
                <option value="3">21:30 - 22:30</option>
                <option value="3">22:30 - 23:30</option>
              </select>
            </div>
         
            <!-- Message input-->
            <div class="col-12">
                <label class="form-label" for="message"><strong>Messaggio</strong></label>
                <textarea rows="5" class="form-control" id="message" placeholder="Suggeriamo di specificare la presenza di intolleranze e allergie ..." required></textarea>
            </div>

            <div class="col-12">
            <div class="form-check form-switch">
            <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckDefault">
            <label class="form-check-label" for="flexSwitchCheckDefault">Inviando questo nodulo acconsento al trattamento dei miei dati</label>
            </div>

            <div class="col-12 text-center mt-4">
              <button class="btn btn-warning btn-lg" id="submitButton" type="submit">Send</button>
            </div>

          </form>
        </div>   
